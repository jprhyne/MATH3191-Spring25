\newcommand{\instring}[4]{%
    % \instring{<pattern>}{<string>}{<true>}{<false>}
    %   pattern = sought after string
    %   string  = string to search
    \ifnum\pdfmatch{#1}{#2}=1
        #3%
    \else
        #4%
    \fi
}
% If the filename contains the string "beforeClass", then we don't want to have solutions displayed.
%   Otherwise, we display solutions to example problems
\instring{beforeClass}{\jobname}{%
    \settoggle{showSolutions}{false}
}{%
    \settoggle{showSolutions}{true}
}
